@inherits UmbracoViewPage<BrambleBerry.Kitchen.Models.ViewModels.WishListViewModel>
@using BrambleBerry.Kitchen.Models
@{
    Layout = "OneColumn.cshtml";
}

<div id="primary" class="col-s-6 offset-s-3">
    <h1>@Model.Wishlist.Title</h1>
    <ul>
        @foreach (var item in Model.Wishlist.Items)
        {
            <li>@item.SKU @item.QuantityWanted @item.QuantityPurchased</li>
        }
    </ul>
</div>