        <Product id="2607" parentID="2339" level="4" creatorID="0" sortOrder="20" createDate="2014-08-02T10:14:50" updateDate="2014-08-02T10:14:50" nodeName="Bath Bomb Maker" urlName="bath-bomb-maker" path="-1,1050,1065,2339,2607" isDoc="" nodeType="1059" creatorName="admin" writerName="admin" writerID="0" template="1058" nodeTypeAlias="Product">
